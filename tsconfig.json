{
	"compilerOptions": {
		"lib": [
			"ES2020",
			"ScriptHost",
			"DOM",
			"DOM.Iterable"
		],
		"allowJs": false,
		"jsx": "react",
		"isolatedModules": false,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"sourceMap": true,
		"types": ["react", "react-dom", "core-js"]
	},
	"include": [
		"./src/**/*"
	],
	"typeAcquisition": {
		"enable": false,
	}
}
