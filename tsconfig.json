{
	"compilerOptions": {
		"lib": [
			"ES2020",
			"ScriptHost",
			"DOM",
			"DOM.Iterable"
		],
		"allowJs": false,
		"isolatedModules": false,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"sourceMap": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictFunctionTypes": false,
		"types": ["react", "react-dom"]
	},
	"include": [
		"./src/**/*", "Animal.ts"
	],
	"typeAcquisition": {
		"enable": false,
	}
}
