{
	"presets": [
		[
			"@babel/env",
			{
				"useBuiltIns": false
				// "corejs": {
				// 	"version": 3,
				// 	"proposals": true
				// }
			}
		],
		"@babel/typescript",
		"@babel/react"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				// Defaults
				"absoluteRuntime": false,
				"corejs": 3,
				"helpers": true,
				"regenerator": false,
				"useESModules": false,
				"version": "7.0.0-beta.0"
			}
		],
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	]
}
